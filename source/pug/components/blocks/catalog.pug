section.catalog
  h2 Каталог продукции
  ul.catalog-list
    each item in catalogItems
      if item.isLi
        li.catalog-list__item
          picture
            source(type="image/webp" media="(min-width: 1440px)" srcset=item.webpDesktop)
            source(type="image/webp" media="(min-width: 768px)" srcset=item.webpTablet)
            source(type="image/webp" srcset=item.webpPhone)
            source(media="(min-width: 1440px)" srcset=item.srcsetDesktop)
            source(media="(min-width: 768px" srcset=item.srcsetTablet)
            img(src=item.srcImg srcset=item.srcsetImg loading="lazy" alt=item.imgAlt)
          .description__wrapper
            h3=item.h3
            dl
              .dt__wrapper
                dt Масса
                dd=item.dd1
              .dt__wrapper
                dt Вкус
                dd=item.dd2
              .dt__wrapper
                dt Цена
                dd=item.dd3
          button(class="accept-btn" type="button") Заказать
      else
        li(class="catalog-list__item last-item")
          .svg__wrapper
            button(type="button")
              svg(width="30" height="30")
                use(href="./img/svg/icons.svg#plus-icon")
          .description__wrapper
            h3 Показать еще 100500 товаров
            p На самом деле вкусов гораздо больше!
          button(class="show-btn" type="button") Показать все
